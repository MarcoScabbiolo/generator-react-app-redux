{
  "name": "generator-react-app-redux",
  "version": "0.0.0",
  "description": "React app using Redux",
  "homepage": "https://github.com/MarcoScabbiolo/generator-react-app-redux",
  "author": {
    "name": "Marco Scabbiolo",
    "email": "scabbiolo.marco@gmail.com",
    "url": ""
  },
  "files": ["generators"],
  "main": "generators/app/index.js",
  "keywords": [
    "Yeoman",
    "Generator",
    "React",
    "App",
    "Redux",
    "yeoman-generator"
  ],
  "scripts": {
    "prepublish": "nsp check",
    "pretest": "eslint . --fix",
    "precommit": "lint-staged",
    "test": "jest --coverage"
  },
  "devDependencies": {
    "coveralls": "^2.13.1",
    "eslint": "^4.1.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-config-xo": "^0.18.2",
    "eslint-plugin-prettier": "^2.2.0",
    "husky": "^0.14.3",
    "jest": "^21.0.0",
    "jest-cli": "^21.0.0",
    "lint-staged": "^4.2.1",
    "nsp": "^2.6.3",
    "prettier": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "dependencies": {
    "babel-generator": "^6.26.0",
    "babel-types": "^6.26.0",
    "babylon": "^6.18.0",
    "chai": "^4.1.2",
    "chalk": "^2.1.0",
    "deep-extend": "^0.5.0",
    "escodegen": "^1.9.0",
    "esprima": "^4.0.0",
    "fs-extra": "^4.0.2",
    "generator-node": "^2.3.0",
    "promisify-node": "^0.4.0",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": ["**/__tests__/*.js?(x)"],
    "setupTestFrameworkScriptFile": "./__tests__/_utils/global.js",
    "coveragePathIgnorePatterns": ["/node_modules/", "/__tests__/"]
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "*.json": ["prettier --write", "git add"]
  },
  "eslintConfig": {
    "extends": ["xo", "prettier"],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": ["prettier"]
  },
  "repository":
    "https://github.com/MarcoScabbiolo/generator-react-app-redux.git",
  "license": "Apache-2.0"
}
